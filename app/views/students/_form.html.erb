<% if @student.errors.any? %>
<ul>
	<% @student.errors.full_messages.each do |err| %>
	<li><%= err %></li>
	<% end %>
	</ul>
<% end %>

<%= form_for @student do |f| %>

	<p><%= f.label :age %>
	<%= f.text_field :age %></p>
	<p><%= f.label :location %>
	<%= f.text_field :location %></p>
	<p><%= f.label :victim %>
	<%= f.text_field :victim %></p>
	<p><%= f.submit %></p>

<% end %>